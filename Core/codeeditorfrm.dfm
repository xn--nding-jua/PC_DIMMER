    object Memo2: TMemo
      Left = 0
      Top = 0
      Width = 646
      Height = 80
      Align = alClient
      Lines.Strings = (
        '// Derzeit definierte PC_DIMMER-eigenen Funktion:'
        '// Einen Kanalwert unabh'#228'ngig der Ger'#228'te setzen '
        '    set_absolutchannel(Kanal, Startwert, Endwert, Zeit);'
        '    set_absolutchannel(Kanal, Startwert, Endwert, Zeit,Delay);'
        '// Einen Kanal eines Ger'#228'tes setzen'
        
          '    set_channel(Ger'#228'teID, Ger'#228'tekanal, Startwert, Endwert, Zeit)' +
          ';'
        
          '    set_channel(Ger'#228'teID, Ger'#228'tekanal, Startwert, Endwert, Zeit,' +
          'Delay);'
        '// Einen Kanalwert unabh'#228'ngig der Ger'#228'te abfragen'
        '    get_absolutchannel(Kanal):integer;'
        '// Einen Kanal eines Ger'#228'tes abfragen'
        '    get_channel(Ger'#228'teID, Ger'#228'tekanal):integer;'
        '// Byte-Werte in Prozent umrechnen'
        '    levelstr(Kanalwert):string;'
        '// Eine Szene starten'
        '    startscene(SzenenID);'
        '// Eine Szene stoppen'
        '    stopscene(SzenenID);'
        '// Einen Effekt starten'
        '    starteffect(EffektID);'
        '// Einen Effekt stoppen'
        '    stopeffect(EffektID);'
        '// Variablen speichern und laden'
        '    set_var(1, 2);'
        '    get_var(1, 2);'
        '    set_globalvar(1, 2);'
        '    get_globalvar(1, 2);'
        '// Midi oder DataIn senden'
        'sendmidi(MSG, Data1, Data2)'
        'senddatain(Channel, Value)'
        'get_lastmidi() // returns MSG, Data1, Data2 as char in string'
        
          'get_lastdatain() // returns HighByte, LowByte of Address and Val' +
          'ue as char in string'
        '// Kontrollpanel'
        
          'get_panelbuttoncolor(X,Y):TColor / set_panelbuttoncolor(X, Y, TC' +
          'olor)'
        
          'get_panelbuttontext(X,Y):String / set_panelbuttontext(X, Y, Stri' +
          'ng)'
        ''
        ''
        
          '// Als ID z.B.: SzenenID='#39'{93FC1B07-BC0F-4070-BD73-A1DE250BA2BA}' +
          #39
        
          '// die IDs der Szenen, Effekte und Ger'#228'te k'#246'nnen in den jeweilig' +
          'en Programmteilen angezeigt werden'
        ''
        
          '// Einige Standardfunktionen (viele Delphi-Standardfunktionen si' +
          'nd m'#246'glich)'
        '// Nachricht anzeigen'
        '    ShowMessage('#39'Text'#39');'
        '// Datum und Uhrzeit'
        '    TimeToStr(now);'
        '    DateToStr(now);'
        '// Eingabe von Werten:'
        
          '    IntegerVariable:=StrToInt(InputBox('#39#220'berschrift'#39','#39'Hinweistex' +
          't'#39','#39'Vorgabe'#39'))'
        ''
        '// Abfrage:'
        
          '    if messagedlg('#39'M'#246'chten Sie diese Funktion ausf'#252'hren?'#39',mtConf' +
          'irmation,[mbYes,mbNo],0)=mrYes then'
        '      DoSomething;'
        
          '    if messagedlg('#39'Die nachfolgende Funktion ist gef'#228'hrlich. For' +
          'tfahren?'#39',mtWarning,[mbYes,mbNo],0)=mrYes then'
        '      DoSomething;'
        ''
        '// IF-Then-Else'
        '    if true then else ;'
        '// Case'
        '    case IntegerVariable of'
        '      0: ;'
        '      1..2: ;'
        '      3,5: ;'
        '      4: ;'
        '    end;')
      ScrollBars = ssVertical
      TabOrder = 0
    end
